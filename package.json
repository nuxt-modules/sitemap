{
  "name": "@nuxtjs/sitemap",
  "type": "module",
  "version": "6.0.0-beta.2",
  "packageManager": "pnpm@9.5.0",
  "description": "Powerfully flexible XML Sitemaps that integrate seamlessly, for Nuxt.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/nuxt-modules/sitemap#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-modules/sitemap.git"
  },
  "bugs": {
    "url": "https://github.com/nuxt-modules/sitemap/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:type": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build && npm run client:build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^1.3.9",
    "@nuxt/kit": "^3.12.4",
    "chalk": "^5.3.0",
    "defu": "^6.1.4",
    "h3-compression": "^0.3.2",
    "nuxt-site-config": "^2.2.15",
    "nuxt-site-config-kit": "^2.2.15",
    "ofetch": "^1.3.4",
    "pathe": "^1.1.2",
    "pkg-types": "^1.1.3",
    "radix3": "^1.1.2",
    "semver": "^7.6.3",
    "sirv": "^2.0.4",
    "site-config-stack": "^2.2.15",
    "ufo": "^1.5.4"
  },
  "devDependencies": {
    "@nuxt/content": "^2.13.2",
    "@nuxt/eslint-config": "^0.5.0",
    "@nuxt/module-builder": "0.8.3",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxt/ui": "^2.18.4",
    "@nuxtjs/i18n": "8.4.0",
    "@nuxtjs/robots": "4.0.2",
    "bumpp": "^9.5.1",
    "eslint": "9.9.0",
    "eslint-plugin-n": "^17.10.2",
    "execa": "^9.3.1",
    "nuxt": "^3.12.4",
    "typescript": "5.5.4",
    "vitest": "^2.0.5"
  },
  "build": {
    "externals": [
      "h3",
      "std-env",
      "nitropack",
      "consola"
    ]
  }
}
